        "@playwright/test": "^1.48.0",
        "@svgr/webpack": "^8.1.0",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@types/jest": "^30.0.0",
        "@types/jest-axe": "^3.5.9",
        "@types/node": "^24",
        "@types/nodemailer": "^7.0.1",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "babel-jest": "^30.0.5",
