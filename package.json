{
    "name": "skyboundstudio",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "npm install && next dev",
        "update": "npx ncu -u && npm install",
        "lint": "next lint",
        "test": "jest --config tests/jest.config.ts",
